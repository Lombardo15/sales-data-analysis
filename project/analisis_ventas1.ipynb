    "\n",
    "print(\"=\"*80)\n",
    "print(\"KPI: VENTAS MENSUALES\")\n",
    "print(\"=\"*80)\n",
    "display(ventas_mes)\n",
    "print(f\"\\nMes con mayores ventas: {ventas_mes['Ventas Totales'].idxmax()} - ${ventas_mes['Ventas Totales'].max():,.2f}\")\n",
