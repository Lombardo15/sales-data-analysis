   "id": "kpi-9",
   "metadata": {},
   "source": [
    "### 5.9 KPI: Concentraci칩n de Ventas (Regla 80/20)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "kpi-pareto",
   "metadata": {},
   "outputs": [],
   "source": [
    "# An치lisis de Pareto: 쯈u칠 porcentaje de productos genera el 80% de las ventas?\n",
    "productos_ventas = df.groupby('producto')['ventas_producto'].sum().sort_values(ascending=False)\n",
    "productos_ventas_acum = productos_ventas.cumsum()\n",
    "porcentaje_acumulado = (productos_ventas_acum / productos_ventas.sum() * 100)\n",
    "\n",
    "productos_80 = (porcentaje_acumulado <= 80).sum()\n",
    "total_productos = len(productos_ventas)\n",
    "porcentaje_productos_80 = (productos_80 / total_productos * 100)\n",
    "\n",
    "print(\"=\"*80)\n",
    "print(\"KPI: CONCENTRACI칍N DE VENTAS (Principio de Pareto)\")\n",
    "print(\"=\"*80)\n",
    "print(f\"Total de productos 칰nicos: {total_productos}\")\n",
    "print(f\"Productos que generan el 80% de ventas: {productos_80} ({porcentaje_productos_80:.1f}%)\")\n",
    "print(f\"\\nInsight: El {porcentaje_productos_80:.1f}% de los productos genera el 80% de las ventas\")\n",
    "print(\"=\"*80)"
   ]
